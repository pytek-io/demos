    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Render",
            "type": "python",
            "request": "launch",
            "module": "render.kernel",
            "args": [
                "--port",
                "8080"
            ],
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "/Users/francois/work/render/render:/Users/francois/work/render/libraries"
            },
            "cwd": "/Users/francois/work/render"
        },
    ]
}